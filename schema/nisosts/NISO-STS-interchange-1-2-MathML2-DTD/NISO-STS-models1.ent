<!-- ============================================================= -->
<!--                    OVER-RIDES OF ATTRIBUTE VALUE LISTS        -->
<!-- ============================================================= -->

<!--                    CITED STANDARD TYPES                       -->
<!--                    Possible values for the @type attribute
                        on the <std> element.                      -->
<!ENTITY % cited-std-type-values
                        "dated | multipart | undated | custom"       >


<!--                    DEFAULT TYPE OF CROSS(X)-REFERENCE         -->
<!--                    Used to say to what the reference is pointing.
                        May be used for type-specific processing or
                        validation. Values are, for example:
                        Affiliation "aff" and Figure "fig", and
                        Bibliographic ref (to a citation.          -->
<!ENTITY % ref-types    "abbreviation | aff | app | author-notes | 
                         award | bibr | bio | boxed-text | 
                         chem | collab | contrib | corresp |
                         disp-formula | fig | fn | kwd | list |
                         plate | scheme | sec | statement |
                         supplementary-material |
                         table | table-fn |
                         other | custom"                             >
 

<!--                    FORM TYPE VALUES                           -->
<!--                    Possible values for the @form-type attribute
                        on many of the block display elements.     -->
<!ENTITY % form-type-values
                        "blank | exemplar | instructions | 
                          unspecified | custom"                      >
 

<!--                    IS THIS ELEMENT A FORM VALUES              -->
<!--                    Possible values for the @is-form attribute
                        on many of the block display elements.     -->
<!ENTITY % is-form-values
                        "form | noform"                              >


<!--                    TERM STATUS TYPES                          -->
<!--                    Possible values for the @term-status attribute
                        on the <term> element.                     -->
<!ENTITY % term-status-values
                       "preferred | non-preferred | admitted"        >


<!--                    TERM TYPE TYPES                            -->
<!--                    Possible values for the @term-type attribute
                        on the <term> element (taken from TBX)     -->
<!ENTITY % term-type-values
                        "acronym | abbreviation | fullForm | symbol | 
                         variant | formula | equation | translation" >

<!-- ============================================================= -->
<!--                    OVER-RIDES OF JATS METADATA MODELS         -->
<!-- ============================================================= -->               


<!--                    ABSTRACT MODEL                             -->
<!--                    Abstracts may contain one or more paragraphs
                        (as a typical abstract does) or titled
                        sections, as is done in many physics
                        articles.
                        Unlike the default model in the base DTD
                        Suite, this <abstract> requires that all
                        sections in the abstract start with titles.
                        NISO STS change: add <editing-instruction> -->
<!ENTITY % abstract-model
                        "( (%editing-instructions.class;)*,
                           label?, title?,
                           (%just-para.class;)*, (%sec.class;)*)"    >


<!--                    AFFILIATION ELEMENTS                       -->
<!--                    Elements for use in the <aff> element      -->
<!ENTITY % aff-elements "| %address.class; | %address-link.class; |
                         %article-link.class; | %break.class; |
                         %emphasis.class; | %label.class; |
                         %inline-text.class; |
                         %simple-link.class; | %subsup.class;"       >


<!--                    COPYRIGHT STATEMENT ELEMENTS               -->
<!--                    Content model for <copyright-statement>    -->
<!ENTITY % copyright-statement-elements
                        "| %address-link.class; | %emphasis.class; |
                         %inline-text.class; |
                         %phrase-content.class; | %subsup.class;"    >

<!--                    FUNDING STATEMENT ELEMENTS                 -->
<!--                    Model for the <funding-statement> element  -->
<!ENTITY % funding-statement-elements
                        "| %address-link.class; | %emphasis.class; |
                         %inline-text.class; |
                         %phrase-content.class; |
                         %subsup.class;"                             >

<!--                    INSTITUTION NAME ELEMENTS                  -->
<!--                    Elements for use in the <institution>
                        element                                    -->
<!ENTITY % institution-elements
                        "| %break.class; | %emphasis.class; |
                         %inline-text.class; |
                         %subsup.class;"                             >


<!--                    KEYWORD CONTENT ELEMENTS                   -->
<!--                    The elements that can be included along with
                        data characters inside the content model of
                        a keyword <kwd>.                           -->
<!ENTITY % kwd-elements
                        "| %emphasis.class; | %inline-text.class; |
                         %phrase-content.class; | %subsup.class;"    >
     

<!--                    KEYWORD GROUP MODEL                        -->
<!--                    Content model for a <kwd-group> element
                        NISO STS change: add <editing-instruction> -->
<!ENTITY % kwd-group-model
                        "( (%editing-instructions.class;)*,
                           label?, title?,
                           (%kwd.class;)+ )"                         >


<!--                    LINK ELEMENTS                              -->
<!--                    Elements for use in the linking elements
                        such as <xref>, <target>, and <ext-link>   -->
<!ENTITY % link-elements
                        "| %emphasis.class; | %inline-text.class; |
                         %phrase-content.class; | %subsup.class;"    >


<!--                    PUB DATE MODEL                             -->
<!--                    Content model for the <pub-date> element   -->
<!--                    In NISO STS, dates within the <pub-date>  
                        element are given in ISO 8601 form, so no 
                        sub-part tagging is required (or allowed)
                        and no @iso-8601 attribute is required.    -->
<!ENTITY % pub-date-model 
                        "(#PCDATA)"                                  >



<!--                    PUBLISHER'S LOCATION ELEMENTS              -->
<!--                    Elements for use in the Publisher Location
                        <publisher-loc> element                    -->
<!ENTITY % publisher-loc-elements
                        "| %address.class; | %address-link.class; |
                         %emphasis.class; | 
                         %inline-text.class; | %subsup.class;"       >


<!--                    RELATED ARTICLE ELEMENTS                   -->
<!--                    Elements allowed inside <related-article>  -->
<!ENTITY % related-article-elements
                        "| %emphasis.class; | %journal-id.class; | 
                         %inline-text.class; |
                         %phrase-content.class; |
                         %references.class; |  %subsup.class;"       >


<!--                    RELATED OBJECT ELEMENTS                    -->
<!--                    Elements allowed inside <related-object>   -->
<!ENTITY % related-object-elements
                        "| %emphasis.class; | 
                         %inline-text.class; |
                         %phrase-content.class; |
                         %references.class; |  %subsup.class;"       >


<!--                    CONTENT MODEL FOR A SECTION METADATA       -->
<!--                    This wrapper element holds section-level 
                        metadata, such as specific keywords,  that is 
                        different from the metadata for the rest of 
                        the stadnard.                              -->
<!ENTITY % sec-meta-model
                        "( (%kwd-group.class;)*, 
                           (%subj-group.class;)* )"                  >


<!--                    TITLE-PART ELEMENTS                        -->
<!--                    The %title-part-elements; parameter entity 
                        lists the elements legal in any part of 
                        a title.
                        Developer's Note: used in <subtitle>         
                        model below.                               -->
<!ENTITY % title-part-elements 
                       "| %address-link.class; |
                         %article-link.class; | %emphasis.class; |
                         %inline-display.class; |
                         %inline-math.class; | %math.class; |
                         %phrase.class; | %simple-link.class; |
                         %subsup.class; |
                         %break.class; |
                         %citation.class;"                           >


<!--                    TRANSLATED ABSTRACT MODEL                  -->
<!--                    Content model for an <trans-abstract>
                        element                                    
                        NISO STS change: add <editing-instruction> -->
<!ENTITY % trans-abstract-model
                        "( (%editing-instructions.class;)*,
                           label?, title?,
                           (%just-para.class;)*, (%sec.class;)*)"    >


<!--                    SIGNATURE ELEMENTS                         -->
<!--                    Elements to be mixed with data characters
                        inside the content model for the
                        <sig> element.                             -->
<!ENTITY % sig-elements "| %break.class; | %emphasis.class; |
                         %subsup.class; | %phrase-content.class; |
                         %inline-display-noalt.class; |
                         %just-base-display-noalt.class;"            >


<!--                    STANDARDS ORGANIZATION ELEMENTS            -->
<!--                    The elements that can be included along with
                        data characters inside the content model of
                        <std-organization>.                        -->
<!ENTITY % std-organization-elements 
                        "| %emphasis.class; | %inline-text.class; |
                         %institution-wrap.class; | %subsup.class;"  >


<!--                    SUBTITLE ELEMENTS                          -->
<!--                    The elements that can be included along with
                        data characters inside the content model of
                        a paragraph <subtitle>.
                        An earlier version of this tag set used the
                        parameter entity %title-elements; for many
                        title elements including <subtitle>.       -->
<!ENTITY % subtitle-elements
                        "%title-part-elements;"                      >

<!-- ============================================================= -->
<!--                    OVER-RIDES OF JATS SECTION MODELS          -->
<!-- ============================================================= -->


<!--                    CONTENT MODEL FOR A STRUCTURAL SECTION     -->
<!--                    The model for a section that requires that 
                        a <title> or a <label> must be present. 
                        ISO STS Change: Remove <sec-meta>
                        NISO STS Change: Add <editing-instruction>,
                        Put <sec-meta> back in.                    -->
<!ENTITY % sec-model    "( (%editing-instructions.class;)*, sec-meta?, 
                           ( (label, title?) | title ),
                             (%para-level; | %sec-back.class;)*, 
                             ( (%sec.class;)+, 
                               (%sec-back.class; | %all-notes.class;)* 
                            )? 
                         )"                                          >

<!--  ( (x, y*) | y+)  -->

<!--<!ENTITY % sec-model    "( editing-instruction*, sec-meta?, 
                           ( (label, title?) | title ),
                           ( ((%para-level; | %sec-back.class;)*, 
                               ( (%sec.class;)+, 
                                (fn-group | glossary |  ref-list |  
                                non-normative-note | normative-note | 
                                non-normative-example | 
                                normative-example | notes-group)* 
                               )*
                             ) |
                              ( (%sec.class;)+, 
                                (%sec-back.class; |  
                                 %all-notes.class;)* 
                              )+
                           )
                            
                         )"                                        > --> 

<!-- ============================================================= -->
<!--                    OVER-RIDES OF JATS JOURNAL PUBLISHING      -->
<!--                    STRUCTURAL MODELS                          -->
<!-- ============================================================= -->

<!--                    ACKNOWLEDGMENTS MODEL                      -->
<!--                    Content model for the <ack> element        -->
<!ENTITY % ack-model    "( label?, title?,
                           (%para-level; | %sec-back.class;)*, 
                           ( (%sec.class;)+,  
                             (%sec-back.class; | %all-notes.class;)*
                           )? 
                         )"                                          >


<!--                    BIOGRAPHY MODEL                            -->
<!--                    Content model for the <bio> element        -->
<!ENTITY % bio-model    "( label?, title?,
                           (%para-level; | %sec-back.class;)*, 
                           ( (%sec.class;)+,  
                             (%sec-back.class; | %all-notes.class;)*
                           )? 
                         )"                                          >


<!--                    CAPTION MODEL                              -->
<!--                    Content model for the <caption> element 
                        NISO STS Change: add <editing-instruction> -->
<!ENTITY % caption-model
                        "( (%editing-instructions.class;)*,
                           title?,
                           (%just-para.class;)* )"                   >


<!--                    APPENDIX MODEL                             -->
<!--                    Content model for the <app> element.       -->
<!--                    ISO STS Change" Added annex-type. 
                        NISO STS Change: add <editing-instruction> -->
<!ENTITY % app-model    " ( (%editing-instructions.class;)*, 
                            sec-meta?, 
                            ( (label, annex-type?, title?) |
                              (annex-type?, title) ),
                            (%para-level; | %sec-back.class;)*,  
                            ( (%sec.class;)+, 
                              (%sec-back.class; | %all-notes.class;)* 
                            )?, 
                            permissions?)"                             > 

<!--old app model
                           "( (%editing-instructions.class;)*,
                            ( (label, annex-type?, title?) |
                              (annex-type?, title) ),
                          ( ( (%para-level; | %ref-list.class;)+, 
                              (%sec-level; | %xinclude.class;)*) |
                            (%sec-level; | %xinclude.class;)+ ),
                          ( (%sec-back-matter-mix;),    
                            (%all-notes.class;)* )*, 
                          permissions?)"                             >


--><!--                    APPENDIX GROUP MODEL                       -->
<!--                    Content model for the <app-group> element  -->
<!ENTITY % app-group-model
                        "(label?, title?, 
                          (%abstract.class;)*, 
                          (%kwd-group.class;)*, (%subj-group.class;)*,
                          (%para-level;)*,
                          (%app.class; | %ref-list.class; |
                           %xinclude.class;)*)"                      >


<!--                    LABEL ELEMENTS                             -->
<!--                    The elements that can be included along with
                        data characters inside the content model of
                        the <label> element.
                        Added %phrase.content to allow named-content
                        within a label.                            -->
<!ENTITY % label-elements
                        "| %emphasis.class; | %inline-display.class; |
                         %inline-math.class; | 
                         %phrase-content.class; | 
                         %simple-link.class; | %subsup.class;"       >


<!--                    NOTES MODEL                                -->
<!--                    Content model for the <notes> element      -->
<!ENTITY % notes-model  "(label?, title?,
                          (%para-level; | %sec-back.class;)*, 
                          ( (%sec.class;)+, 
                            (%sec-back.class; | %all-notes.class;)* 
                          )? 
                         )"                                          >


<!--                    REFERENCE ITEM MODEL                       -->
<!--                    Content model for the <ref> element
                        ISO STS Change: Removed %note.class; (<note>)
                        NISO STS Change: add <editing-instruction>
                        and all normative and non-normative notes 
                        and examples.                              -->
<!ENTITY % ref-model    "( (%editing-instructions.class;)*,
                           label?,  
                           (%citation.class; | %all-notes.class;)+ )">


<!-- ============================================================= -->
<!--                    OVER-RIDES OF JATS JOURNAL PUBLISHING      -->
<!--                    ELEMENTS FOR CITATIONS                     -->
<!-- ============================================================= -->


<!--                    CITATION ELEMENTS                          -->
<!--                    Content for both types of citation. These
                        elements may be mixed with #PCDATA in the
                        <mixed-citation> element (in which all
                        punctuation and spacing are left intact), and
                        they also constitute the choices that can be
                        used to form the all-element-content of the
                        <element-citation> element (in which
                        punctuation and spacing are removed).      -->
<!--                    Added break.class and simple-link.class    -->
<!ENTITY % citation-elements
                        "%break.class; | 
                         %emphasis.class; | %inline-display.class; |
                         %inline-math.class; | %label.class; |
                         %phrase.class; | %references.class; |
			             %simple-link.class; | %subsup.class;"       >


<!--                    STANDARD ELEMENTS                          -->
<!--                    The elements that can be included along with
                        data characters inside the content model of
                        <std>
                        Added <std-ref>, <title>, and <data-title>.
                        Design Note: this inline mix begins with
                        an OR bar inside rendition-plus.           -->
<!ENTITY % std-elements "| %std-content.class; | 
                         %emphasis.class; | %inline-text.class; |
                         %phrase-content.class; | %subsup.class;"    >


<!-- ============================================================= -->
<!--                    OVER-RIDE BLOCK DISPLAY ELEMENTS           -->
<!-- ============================================================= -->


<!--                    ARRAY CONTENT MODEL                        -->
<!--                    Content model for the <array> element      -->
<!--                    ISO STS Change: Made this use table.class,
                        not tbody.class  
                        NISO STS change: add <editing-instruction>
                        Remarks: <table> is non-preferred, use 
                        <tbody>                                    -->
<!ENTITY % array-model  "( (%editing-instructions.class;)*,
                           label?,
                           (%access.class; | %address-link.class;)*,
                           (%just-base-display.class; |
                             %table.class; | %tbody.class;)*,
                           (%display-back-matter.class;)* )"         >


<!--                    BOXED TEXT MODEL                           -->
<!--                    Complete content model for the boxed text
                        element <boxed-text>.
                        ISO STS Change: Remove <sec-meta> 
                        Note: Adding <editing-instruction>
                        would blow determinism rules, since
                        <editing-instruction> is part of
                        para-level stuff.                          -->
<!ENTITY % boxed-text-model
                        "( (%id.class;)*, sec-meta?, label?,
                           (%caption.class;)?, (%legend.class;)*,
                           (%para-level; | %sec-back.class;)*, 
                           ( (%sec.class;)+, 
                             (%sec-back.class; | %all-notes.class;)*
                           )?,
                           (%display-back-matter.class;)* )"         >


<!--                    CHEMICAL STRUCTURE ELEMENTS                -->
<!--                    Those elements that may mix with the data
                        characters inside a Chemical Structure
                        <chem-struct>                              -->
<!ENTITY % chem-struct-elements
                        "| %access.class; | %address-link.class; |
                         %break.class; |
                         %emphasis.class; | %label.class; |
                         %inline-text.class; |
                         %list.class; | %math.class; |
