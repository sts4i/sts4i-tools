<!--                    DEFINITION LIST: TERM ATTRIBUTES           -->
<!--                    Attributes for the <term> element.
                        In this Tag Set, used as part of
                        <term-display> as well as ordinary
                        definition lists.                
                        NISO STS: Adds @term-status, @term-type,
                        and copies the NISO JATS 1.1d1 vocab 
                        attributes.                               -->
<!ENTITY % term-atts
            "%jats-common-atts;
             term-status
                        (%term-status-values;)            #IMPLIED
             term-type  (%term-type-values;)              #IMPLIED
             vocab      CDATA                             #IMPLIED
             vocab-identifier
                        CDATA                             #IMPLIED
             vocab-term CDATA                             #IMPLIED
             vocab-term-identifier
                        CDATA                             #IMPLIED
             rid        IDREFS                            #IMPLIED
             specific-use
                        CDATA                             #IMPLIED
             xml:lang   NMTOKEN                           #IMPLIED"  >

<!-- ============================================================= -->
<!--                    TABLES AND INDEX REASSIGNS TO REMOVE 'X'   -->
<!-- ============================================================= -->

<!--                    INDEX MODEL                                -->
<!--                    Content model for the Structural Index
                        <index> element.
                        NISO STS removed the <x> element.          -->
<!ENTITY % index-model  "(index-title-group?, (%para-level;)*,
                          (%index-entry.class; | 
                           %index-div.class;)+  )"                  >


<!--                    INDEX DIVISION MODEL                       -->
<!--                    Content model for the Structural Index
                        <index-entry> element. 
                        NISO STS removed the <x> element.          -->
<!ENTITY % index-div-model
                        "(index-title-group?, (%para-level;)*,
			              (%index-entry.class; | 
                          %index-div.class;)+  )"                    >


<!--                    INDEX ENTRY MODEL                          -->
<!--                    Content model for the Structural Index
                        <index-entry> element.  
                        NISO STS removed the <x> element.          -->
<!ENTITY % index-entry-model
                        "(term, (%index-entry.class; |
                           %struct-link.class; | 
                           %see-also-see-entry.class;)*  )"          >
 
<!-- ============================================================= -->
<!--                    OVERRIDES FOR BITS TOC MODELS              -->
<!--                    SIMPLIFY!                                  -->
<!-- ============================================================= -->


<!--                    TABLE OF CONTENTS Model                    -->
<!--                    Content model for the <toc-group> element  -->
<!ENTITY % toc-group-model
                        "(toc-title-group?, 
                          (%nothing-but-para.class;)*,
                          (%toc.class;)+)"                           >


<!--                    TABLE OF CONTENTS Model                    -->
<!--                    Content model for the <toc> element     
                        NISO STS removed the <x> element.          -->
<!ENTITY % toc-model    "(toc-title-group?, 
                          (%nothing-but-para.class;)*,
                          (%toc-entry.class; | %toc-div.class;)+,
                          (%fn-group.class;)* )"                     >


<!--                    TABLE OF CONTENTS DIVISION MODEL           -->
<!--                    Content model for the <toc-div> element.   -->
<!ENTITY % toc-div-model
                        "(toc-title-group?,
                          (%nothing-but-para.class;)*,
                          (%struct-link.class;)?,
                          (%toc-entry.class; | %toc-div.class;)+ )"  >


<!--                    TABLE OF CONTENTS ENTRY MODEL              -->
<!--                    Content model for the <toc-entry> element. -->
<!ENTITY % toc-entry-model
                        "(label?, title*, subtitle*,
                          (%struct-link.class;)?,
                          (%toc-entry.class;)* )"                    >


<!--                    TOC TITLE GROUP MODEL                      -->
<!--                    Content model for <toc-title-group>.       -->
<!ENTITY % toc-title-group-model
                        "(label?, title?, subtitle*)"                >

